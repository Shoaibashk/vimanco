{
  "name": "vimanco",
  "displayName": "vimanco",
  "description": "Shoaib's Key bindings for Visual Studio Code",
  "version": "0.0.1",
  "icon": "Assets/keybinding.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shoaibashk/vimanco"
  },
  "publisher": "shoaibashk",
  "license": "Apache 2.0",
  "extensionDependencies": [
    "vscodevim.vim"
  ],
  "keywords": [
    "vim",
    "vi",
    "shoaibashk",
    "vimanco"
  ],
  "homepage": "https://github.com/Shoaibashk/vimanco",
  "bugs": {
    "url": "https://github.com/Shoaibashk/vimanco/issues"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vimanco.updateVim",
        "title": "Update Vim with Vimanco"
      }
    ],
    "configuration": {
      "title": "ImKey",
      "properties": {
        "ImKey.UseSystemClipboard": {
          "type": "boolean",
          "default": true,
          "description": "Enable use system clipboard."
        },
        "ImKey.EasyMotion": {
          "type": "boolean",
          "default": true,
          "description": "Enable Easy Motion"
        },
        "ImKey.IncSearch": {
          "type": "boolean",
          "default": true,
          "description": "Enable Incremental Search in Vim"
        },
        "ImKey.HlSearch": {
          "type": "boolean",
          "default": true,
          "description": "Enable Highlight Search in Vim"
        },
        "ImKey.InsertModeKeyBindings": {
          "type": "array",
          "default": [
            {
              "before": [
                "j",
                "j"
              ],
              "after": [
                "<Esc>"
              ]
            }
          ],
          "description": "Enable Insert Mode Key binding"
        },
        "ImKey.NormalModeKeyBindingsNonRecursive": {
          "type": "array",
          "default": [
            {
              "before": [
                "<leader>",
                "d"
              ],
              "after": [
                "d",
                "d"
              ]
            },
            {
              "before": [
                "<C-n>"
              ],
              "commands": [
                ":nohl"
              ]
            },
            {
              "before": [
                "K"
              ],
              "commands": [
                "lineBreakInsert"
              ],
              "silent": true
            }
          ],
          "description": "Normal mode Keybinding"
        },
        "ImKey.Leader": {
          "type": "string",
          "default": "<space>",
          "description": "Enable Easy Motion"
        },
        "ImKey.HandleKeys": {
          "type": "object",
          "default": {
            "<C-a>": false,
            "<C-f>": false,
            "<C-x>": false,
            "<C-c>": false,
            "<C-v>": false
          },
          "description": "Allow HandleKeys in Visual Mode"
        }
      }
    },
    "keybindings": [
      {
        "key": "ctrl+n",
        "command": "editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+j",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus "
      },
      {
        "key": "ctrl+k",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus "
      },
      {
        "key": "ctrl+`",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
      },
      {
        "key": "ctrl+k",
        "command": "-extension.vim_ctrl+k",
        "when": "editorTextFocus && vim.active && vim.use<C-k> && !inDebugRepl"
      },
      {
        "key": "shift+alt+k",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+alt+j",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+ctrl+k",
        "command": "editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "shift+ctrl+j",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
      },
      {
        "key": "ctrl+t",
        "command": "-extension.vim_ctrl+t",
        "when": "editorTextFocus && vim.active && vim.use<C-t> && !inDebugRepl"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "pkgupdate": "gulp updateTag & vsce package"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-cli": "^2.3.0",
    "gulp-git": "^2.10.1",
    "gulp-tag-version": "^1.3.1",
    "minimist": "^1.2.8",
    "plugin-error": "^2.0.1",
    "yargs": "^17.7.2",
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "ts-loader": "^9.5.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.6"
  }
}
